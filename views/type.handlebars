<section class="section has-background-light is-clipped">
    {{#each type}}
    <a href="/type/{{this.type}}">
        <div class="container">
            <h2 class="title mb-16 mb-24-tablet">{{this.type}}</h2>
            <hr>
            <hr>
            <div class="mb-20 columns is-multiline">
                {{#each product}}
                {{> product}}
                {{/each}}
            </div>
        </div>
    </a>
    {{/each}}
</section>